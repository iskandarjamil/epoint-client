{
    "name": "iskandarjamil/epoint-client",
    "description": "The Epoint System API Client Library.",
    "keywords": ["Epoint System", "Epoint", "Client", "Library"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Iskandar Jamil",
            "email": "iskandar.jam@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "optimize-autoloader": true,
    "require": {
        "php": "~7.1.3||~7.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0",
        "vlucas/phpdotenv": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "EpointClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EpointClient\\Test\\": "tests/"
        },
        "files": [
            "src/helpers/global.php"
        ]
    }
}
